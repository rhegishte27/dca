<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suppressions PUBLIC
	"-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
	"https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
	<!-- Suppress files under target -->
	<suppress checks=".*" files=".*[/\\]target[/\\].*"/>

	<!-- Suppress method naming under test -->
	<suppress checks="MethodName" files=".*[/\\]test[/\\].*"/>

	<!-- Temporary (expires 31/06/2018): disable checks for un-formatted projects -->
	<suppress checks="Translation" files=".*messages_es.properties"/>

	<suppress checks="MethodName" files=".*Test.java"/>
</suppressions>
